<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE symbol-and-basic-behavior-files SYSTEM "../symbol-and-basic-behavior-files.dtd">
<option xmlns="http://www.xabsl.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="striker" initial-state="__initial">
  &my_symbols;
  &my_basic_behaviors;
  &options;
  <state name="__initial">
    <subsequent-basic-behavior ref="go_to">
      <set-parameter ref="go_to.x">
        <minus>
          <decimal-input-symbol-ref ref="ball.x"/>
          <decimal-value value="8"/>
        </minus>
      </set-parameter>
      <set-parameter ref="go_to.y">
        <decimal-value value="11"/>
      </set-parameter>
    </subsequent-basic-behavior>
    <decision-tree>
      <transition-to-state ref="__initial"/>
    </decision-tree>
  </state>
</option>
