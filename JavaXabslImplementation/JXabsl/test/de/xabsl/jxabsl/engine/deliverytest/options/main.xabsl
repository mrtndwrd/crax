

/** main option */


option main {

  initial state getNext {
   decision {
    goto deliver;
   }
	action {
	 packet = packet + 1;
    }
  }
  
  state deliver {
   decision {
    if (action_done) {
     if (packet < getNrPackets - 1) {
       goto getNext;
     } else {
       goto done;
     } 
    } else {
     stay;
    }
   
   }
   
   action {
    deliver();
   }
  }

  target state done {
	decision { stay; }
	action {}
  }
}

